#@include fixture/documents.jsons
{
  "type": "search",
  "dataset": "Droonga",
  "body": {
    "queries": {
      "result": {
        "source": "Sections",
        "condition": [
          "&&",
          "title @ 'groonga'",
          "title @ 'engine'"
        ],
        "sortBy": ["_key"],
        "output": {
          "elements": [
            "startTime",
            "elapsedTime",
            "count",
            "attributes",
            "records"
          ],
          "limit": -1,
          "attributes": ["title"]
        }
      }
    }
  }
}
